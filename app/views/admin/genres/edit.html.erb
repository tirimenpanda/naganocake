<div class="container">
	<h3>ジャンル編集画面</h3>
			<P>
				<h4>ジャンル</h4>
					<%= form_for(@genre.name) do |f| %>
						<%=f.text_field :genres_name %>
						<%= f.submit "変更を保存する", class: "btn btn-primary" %>
							<label><%= f.radio_button :is_displayed, true %>有効</label>
							<label><%= f.radio_button :is_displayed, false %>無効</label>
					<% end %>

			</p>

</div>

