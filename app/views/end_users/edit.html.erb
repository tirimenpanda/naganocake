<html>
<body>
	<!-- URL違うところに飛んじゃう -->
	<div class="container">
		<div class="row">
			<div class="my_page">
				<h3>マイページ</h3>
					<h3><strobg>登録情報</strobg></h3>
						<div class="btn-wrapper">
							<!-- <div class="row"> -->
								<p class="btn btn-info col-xs-2" href="/end_users/edit">編集する</p>
							<!-- </div> -->
							<!-- <div class="row"> -->
								<p class="btn btn-info col-xs-4" href="/end_users/password/new">パスワードを変更する</p>
							<!-- </div> -->
						</div>

					<table class="table">
						<thead></thead>
						<tbody>
							<tr>
								<th>氏名</th>
								<th>
									<label for="current_end_user_name"></label>
									   <%#= f.text_field :first_name, :last_name %>
								</th>
							</tr>
							<tr>
								<th>カナ</th>
								<th>
									<label for="current_end_user_name"></label>
										<%#= f.text_field :first_name(kana), :last_name(kana) %>
								</th>
							</tr>
							<tr>
								<th>郵便番号</th>
								<th>
									<label for="current_end_user_postal_code"></label>
										<p>〒</p><%#= f.text_field :postal_code %>
								</th>
							</tr>
							<tr>
								<th>住所</th>
								<th>
									<label for="current_end_user_adress"></label>
										<%#= f.text_field :address %>
								</th>
							</tr>
							<tr>
								<th>電話番号</th>
								<th>
									<label for="current_end_user_phone_number"></label>
										<%#= f.text_field :phone_number %>
								</th>
							</tr>
							<tr>
								<th>メールアドレス</th>
								<th>
									<label for="current_end_user_email"></label>
										<%#= f.text_field :email %>
								</th>
							</tr>
						</tbody>
					</table>
					<!-- <div class="row"> -->
						<div class="btn-wrapper">
							<h3><strong>配送先</strong></h3>
								<p class="btn btn-info" href="/addresses">一覧を見る</p>
						</div>
						<h3><strong>注文履歴</strong></h3>
								<p class="btn btn-info" href="/addresses">一覧を見る</p>


			</div>
		</div>
	</div>
</body>
</html>

